{
  "name": "gatsby-plugin-all-in-one-accessibility",
  "description": "About It uses the accessibility interface which handles UI and design related adjustments. All in One Accessibility app enhances your Gatsby website accessibility to people with hearing or vision impairments, motor impaired, color blind, dyslexia, cognitive & learning impairments, seizure and epileptic, and ADHD problems.",
  "author": "Steven Wenzel <developer3@skynettechnologies.com>",
  "license": "MIT",
  "version": "1.0.11",
  "main": "gatsby-ssr.js",
  "homepage": "https://github.com/skynettechnologies/gatsby-plugin-all-in-one-accessibility#readme",
  "repository": "git@github.com:skynettechnologies/gatsby-plugin-all-in-one-accessibility.git",
  "bugs": {
    "url": "https://github.com/skynettechnologies/gatsby-plugin-all-in-one-accessibility/issues"
  },
  "keywords": [
    "All in One Accessibility",
    "ADA",
    "All in One Accessibility gatsby",
    "All in One Accessibility javascript",
    "analytics.js",
    "gatsby",
    "Gatsby Plugin All in One Accessibility",
    "gatsby-plugin",
    "gatsby-plugin-all-in-one-accessibility"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore tests",
    "build:watch": "yarn build -w",
    "prepare": "yarn build",
    "build-n-pack": "yarn build && yarn pack --filename ./gatsby-plugin-segment-js.tgz"
  },
  "peerDependencies": {
    "gatsby": "^2 || ^3 || ^4 || ^5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^7.32.0",
    "eslint-plugin-mocha": "^9",
    "eslint-plugin-react": "^7.24.0",
    "netlify-plugin-all-in-one-accessibility": "^1.0.5"
  }
}
